{
  "name": "fattunderwriting",
  "version": "0.1.0",
  "description": "a micro service for underwriting use",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf ./dist && tsc --project tsconfig.prodbuild.json && npm run copy-templates",
    "start": "nodemon",
    "stop": "pm2 delete fattunderwriting",
    "release": "pm2 start dist/index.js --name 'fattunderwriting' --no-daemon",
    "test": "jest",
    "test:watch": "jest --watch",
    "docker:build": "docker build -t fattmerchant/underwriting .",
    "docker:run": "docker run -p 3004:3004 -d fattmerchant/underwriting",
    "copy-templates": "cp -r ./src/util/templates ./dist/util/templates"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.{ts}": "eslint --cache --fix",
    "*.{js,ts,css,md}": "prettier --write"
  },
  "author": "daniel walker <daniel@fattmerchant.com>",
  "license": "ISC",
  "dependencies": {
    "@auth0/s3": "^1.0.0",
    "@aws-sdk/client-secrets-manager": "^3.56.0",
